#include <iostream>
#include <vector>
using namespace std;

int main()
{
	//冒泡排序
	vector<int> v;
	v.push_back(12);
	v.push_back(56);
	v.push_back(15);
	v.push_back(59);
	v.push_back(9);
	v.push_back(69);
	v.push_back(36);
	// 12 56 15 59 9 69 36
	int tmp;
	for (int i = 0; i < v.size(); ++i)
	{
		for (int j = 0; j < v.size() - i - 1; ++j)
		{
			if (v[j + 1] <= v[j])
			{
				tmp = v[j + 1];
				v[j + 1] = v[j];
				v[j] = tmp;
			}
		}
	}
	for (int i = 0; i < v.size(); i++)
	{
		cout << v[i] << " ";
	}
	cout << endl;
	system("pause");
	return 0;
}
